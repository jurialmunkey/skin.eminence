<?xml version="1.0" encoding="UTF-8"?>
<!-- Foundation -->
<window id="123">
    <defaultcontrol always="true">5</defaultcontrol>
    <allowoverlay>yes</allowoverlay>
    <zorder>10</zorder>
    <controls>
        <control type="group">
            <posy>210</posy>
            <include>Animation.FadeIn.Up</include>
            <include>Animation.FadeOut</include>
            <animation effect="fade" start="100" end="0" time="400" condition="Window.IsVisible(SliderDialog) | Window.IsVisible(FileBrowser)">Conditional</animation>
            <control type="image">
                <width>1920</width>
                <height>764</height>
                <texture colordiffuse="PanelBG">common/white.png</texture>
                <animation effect="fade" end="90" condition="true">Conditional</animation>
            </control>
            
            <control type="label">
                <description>Header Label</description>
                <textcolor>PanelFG</textcolor>
                <include>DefSettingsHeader</include>
                <label>5</label>
            </control>
 
            <control type="grouplist" id="5">
                <description>Control Area</description>
                <posx>265</posx>
                <posy>150</posy>
                <width>1390</width>
                <height>549</height>
                <itemgap>3</itemgap>
                <orientation>vertical</orientation>
                <pagecontrol>60</pagecontrol>
                <onleft>5</onleft>
                <onright>5</onright>
                <onup>5</onup>
                <ondown>5</ondown>
            </control>

            <control type="button" id="7">
                <description>Default Button</description>
                <align>left</align>
                <texturefocus>noop</texturefocus>
                <texturenofocus>noop</texturenofocus>
                <textcolor>TopBarIcon</textcolor>
                <focusedcolor>Highlight</focusedcolor>
                <disabledcolor>TopBarDisabled</disabledcolor>
                <font>Font-ListInfo</font>
            </control>

            <control type="radiobutton" id="8">
                <description>Default RadioButton</description>
                <align>left</align>
                <radioposx>1318</radioposx>
                <texturefocus>noop</texturefocus>
                <texturenofocus>noop</texturenofocus>
                <textcolor>TopBarIcon</textcolor>
                <focusedcolor>Highlight</focusedcolor>
                <disabledcolor>TopBarDisabled</disabledcolor>
                <textureradioonfocus colordiffuse="Highlight">buttons/radio-on.png</textureradioonfocus>
                <textureradioonnofocus colordiffuse="TopBarIcon">buttons/radio-on.png</textureradioonnofocus>
                <textureradioofffocus colordiffuse="Highlight">buttons/radio-off.png</textureradioofffocus>
                <textureradiooffnofocus colordiffuse="TopBarIcon">buttons/radio-off.png</textureradiooffnofocus>
                <font>Font-ListInfo</font>
            </control>

            <control type="spincontrolex" id="9">
                <description>Default spincontrolex</description>
                <align>left</align>
                <reverse>yes</reverse>
                <texturefocus>noop</texturefocus>
                <texturenofocus>noop</texturenofocus>
                <disabledcolor>TopBarDisabled</disabledcolor>
                <textureup colordiffuse="TopBarIcon" flipy="true">buttons/spin-down.png</textureup>
                <texturedown colordiffuse="TopBarIcon">buttons/spin-down.png</texturedown>
                <textureupfocus colordiffuse="Highlight" flipy="true">buttons/spin-down.png</textureupfocus>
                <texturedownfocus colordiffuse="Highlight">buttons/spin-down.png</texturedownfocus>
                <textcolor>TopBarIcon</textcolor>
                <focusedcolor>Highlight</focusedcolor>
                <font>Font-ListInfo</font>
            </control>

            <control type="sliderex" id="13">
                <description>Default Slider</description>
                <align>left</align>
                <texturefocus>noop</texturefocus>
                <texturenofocus>noop</texturenofocus>
                <textcolor>TopBarIcon</textcolor>
                <focusedcolor>Highlight</focusedcolor>
                <disabledcolor>TopBarDisabled</disabledcolor>
                <font>Font-ListInfo</font>
            </control>

        </control>

    </controls>

</window>
