<?xml version="1.0" encoding="UTF-8"?>
<!-- Foundation -->
<window id="130">
    <defaultcontrol always="true">5</defaultcontrol>
    <allowoverlay>yes</allowoverlay>

    <controls>
        <control type="group">
            <posx>horzpad</posx>
            <posy>topoffset</posy>
            <visible>!Window.IsVisible(SliderDialog) + !Window.IsVisible(FileBrowser) + !Window.IsVisible(LockSettings.xml)</visible>
            <include>Animation.FadeIn</include>
            <include>Animation.FadeOut</include>
            <control type="label" id="2" description="Info Label">
                <include>DefSideLabelCoords</include>
            </control>
        </control>
        <control type="group">
            <posy>160</posy>
            <include>Animation.FadeIn</include>
            <include>Animation.FadeOut</include>
            <visible>!Window.IsVisible(SliderDialog) + !Window.IsVisible(FileBrowser) + !Window.IsVisible(LockSettings.xml)</visible>
            <include>DefSettingsLinesSettings</include>
            <control type="group">
                <include>DefSettingsLeftGroup</include>
                <control type="image">
                    <texture border="12" colordiffuse="PosterHighlight">common/white.png</texture>
                    <height>66</height>
                    <include>DefSettingsLeftGroupWidth</include>
                </control>
                <control type="label" id="102">
                    <posx>30</posx>
                    <description>Header</description>
                    <include>DefSettingsLeftGroupWidth</include>
                    <align>left</align>
                    <label>10034</label>
                    <textcolor>Selected</textcolor>
                </control>
                <control type="image" id="101">
                    <description>profile image</description>
                    
                    <posy>138</posy>
                    <include>DefSettingsLeftGroupWidth</include>
                    <height>342</height>

                    <aspectratio align="center" aligny="center">scale</aspectratio>
                </control>
            </control>
            <control type="grouplist">
                <include>DefSettingsRightGroup</include>
                <ondown>2</ondown>
                <usecontrolcoords>true</usecontrolcoords>
                <control type="label" id="103">
                    <posx>30</posx>
                    <include>DefSettingsRightGroupWidth</include>
                    <align>left</align>
                </control>
            </control>
            <control type="group">
                <posy>69</posy>
                <control type="grouplist" id="5">
                    <include>DefSettingsRightGroup</include>
                    <onleft>5</onleft>
                    <onright>28</onright>
                    <ondown>28</ondown>
                    <onup>5</onup>
                </control>
            </control>
            
            

            <control type="button" id="7">
                <description>Default Button</description>
                <align>left</align>
                <font>Font-ListInfo</font>
                <texturefocus border="12" colordiffuse="PosterHighlight">common/white.png</texturefocus>
            </control>

            <control type="radiobutton" id="8">
                <description>Default Radio Button</description>
                <radioposx>950</radioposx>
                <align>left</align>
                <font>Font-ListInfo</font>
                <texturefocus border="12" colordiffuse="PosterHighlight">common/white.png</texturefocus>
            </control>

            <control type="spincontrolex" id="9">
                <description>Default Spin Control</description>
                <align>left</align>
                <font>Font-ListInfo</font>
                <texturefocus border="12" colordiffuse="PosterHighlight">common/white.png</texturefocus>
            </control>
            
            <control type="grouplist">
                <posx>265</posx>
                <posy>705</posy>
                <width>1390</width>
                <align>center</align>
                <itemgap>128</itemgap>
                <onup>5</onup>
                <orientation>horizontal</orientation>
                <control type="button" id="28">
                    <label>186</label>
                    <include>DefSettingsOKCancelButton</include>
                </control>
                
                <control type="button" id="29">
                    <description>Cancel</description>
                    <label>222</label>
                    <include>DefSettingsOKCancelButton</include>
                </control>
            </control>
            
        </control>
    </controls>

</window>
