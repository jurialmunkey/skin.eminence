<?xml version="1.0" encoding="UTF-8"?>
<window id="100">
    <defaultcontrol always="true">10</defaultcontrol>
    <allowoverlay>yes</allowoverlay>
    <zorder>20</zorder>
    <onunload condition="!Skin.HasSetting(skin.init) | !Skin.HasSetting(skin.init2)">AlarmClock(Skin.Initialization,ReplaceWindow(startup),00:01,silent)</onunload>
    <controls>
        <include>GlobalOverlay</include>
        <control type="group">

            <posy>280r</posy>
            <animation effect="slide" start="0,340" end="0" time="200">WindowOpen</animation>
            <animation effect="slide" end="0,340" start="0" time="200">WindowClose</animation>
            <include>Animation.FadeOut</include>
            
            <control type="image">
                <width>1920</width>
                <height>764</height>
                <texture border="5" colordiffuse="Keyboard-BG">common/white.png</texture>
            </control>
            <control type="image">
                <posx>-10</posx>
                <width>1940</width>
                <height>764</height>
                <texture border="5" colordiffuse="Keyboard-Keys">common/border-inner.png</texture>
            </control>
            
            <control type="label" id="1">
                <posx>120</posx>
                <posy>20</posy>
                <align>left</align>
                <font>Font-Button</font>
                <width>1200</width>
            </control>
            <control type="textbox" id="9">
                <posx>120</posx>
                <posy>90</posy>
                <align>left</align>
                <font>Font-ListInfo-Small</font>
                <width>1100</width>
                <height>120</height>
            </control>
            
            <control type="button" id="11">
                <top>161</top>
                <left>680r</left>
                <width>270</width>
                <onleft>10</onleft>
                <onright>10</onright>
                <include>DefSettingsOKCancelButton</include>
            </control>
            <control type="button" id="10">
                <top>161</top>
                <left>390r</left>
                <width>270</width>
                <onleft>11</onleft>
                <onright>11</onright>
                <include>DefSettingsOKCancelButton</include>
            </control>
        </control>
    </controls>

</window>
